#include <iostream>

int main()
{
    int m=  4, n = 5;
     int **p = new int*[m];
     for(int i = 0; i < m; ++i){
        p[i] = new int[n]{};
     }
     for(int i = 0; i < m; ++i){
        for(int j = 0; j < m; ++j){
            std::cout<< p[i][j]<<"\t";
        }
        std::cout<<"\n";
     }


    return 0;
}
